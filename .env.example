# Discord Webhook URL (必填)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# 雲端模式 (設為 true 啟用自動排程)
CLOUD_MODE=false

# 掃描時間框架 (1h, 4h, 1d)
HARMONIC_TIMEFRAME=1h

# 掃描間隔 (分鐘)
SCHEDULE_INTERVAL=240

# 峰值檢測靈敏度 (數值越大越不敏感)
PEAK_ORDER=10

# K線數據數量
KLINE_LIMIT=500

# 詳細日誌
VERBOSE=true
